#!/usr/bin/env perl

while(<STDIN>){
    if(/\@GRID/){
        print $_;
        $_=<STDIN>;
        ($x,$y,$z)=split;
        printf "%d %d %d\n",$x+1,$y+1,$z+1;
#最初のレイヤはそのまま記憶する。
	for($iy=0;$iy<$y;$iy++){
	    for($iz=0;$iz<$z;$iz++){
		$grid[$iy*($z+1)+$iz]=$v=<STDIN>;
#		print "($iy,$iz)=$v";
		print $v;
		if($iz==0){
		    $z0=$v;
		}
		if($iy==0){
		    $y0[$iz]=$v;
		}
	    }
	    $grid[$iy*($z+1)+$iz]=$z0;
	    if($iy==0){
		$y0[$iz]=$z0;
	    }
#	    print "{$iy,$iz}=$z0";
	    print $z0;
	}
	for($iz=0;$iz<=$z;$iz++){
	    $grid[$iy*($z+1)+$iz]=$y0[$iz];
#	    print "[$iy,$iz]=".$y0[$iz];
	    print $y0[$iz];
	}
	for($ix=1;$ix<$x;$ix++){
	    for($iy=0;$iy<$y;$iy++){
		for($iz=0;$iz<$z;$iz++){
		    $v=<STDIN>;
#		    print "($iy,$iz)=$v";
		    print $v;
		    if($iz==0){
			$z0=$v;
		    }
		    if($iy==0){
			$y0[$iz]=$v;
		    }
		}
		if($iy==0){
		    $y0[$iz]=$z0;
		}
#		print "{$iy,$iz}=$z0";
		print $z0;
	    }
	    for($iz=0;$iz<=$z;$iz++){
#		print "[$iy,$iz]=".$y0[$iz];
		print $y0[$iz];
	    }
	}
	for($iy=0;$iy<=$y;$iy++){
	    for($iz=0;$iz<=$z;$iz++){
#		print "<$iy,$iz>=",$grid[$iy*($z+1)+$iz];
		print $grid[$iy*($z+1)+$iz];
	    }
	}
    }else{
        print $_;
    }
}
exit 0;
